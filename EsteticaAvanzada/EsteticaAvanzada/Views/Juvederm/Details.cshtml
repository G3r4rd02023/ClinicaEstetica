@model EsteticaAvanzada.Data.Entidades.JuvedermAplicacion
@{
    ViewData["Title"] = "Details";
    Layout = null;
}

<div class=" col-md-12">
    <div class=" card">
        <div class=" card-header text-center">
            <h5>Fillers</h5>
        </div>
        <div class=" card-body text-center">
            <div>
                <h6>Detalles de Aplicación</h6>
                <hr />
                <dl class="row">
                    <dt class="col-sm-6">
                        @Html.DisplayNameFor(model => model.Codigo)
                    </dt>
                    <dd class="col-sm-6">
                        @Html.DisplayFor(model => model.Codigo)
                    </dd>
                    <dt class="col-sm-6">
                        @Html.DisplayNameFor(model => model.Producto)
                    </dt>
                    <dd class="col-sm-6">
                        @Html.DisplayFor(model => model.Producto)
                    </dd>
                    <dt class="col-sm-6">
                        @Html.DisplayNameFor(model => model.VolumenML)
                    </dt>
                    <dd class="col-sm-6">
                        @Html.DisplayFor(model => model.VolumenML)
                    </dd>
                    <dt class="col-sm-6">
                        @Html.DisplayNameFor(model => model.ZonasTratamiento)
                    </dt>
                    <dd class="col-sm-6">
                        @Html.DisplayFor(model => model.ZonasTratamiento)
                    </dd>                                     
                    <dt class="col-sm-6">
                        @Html.DisplayNameFor(model => model.Observaciones)
                    </dt>
                    <dd class="col-sm-6">
                        @Html.DisplayFor(model => model.Observaciones)
                    </dd>
                </dl>
            </div>
        </div>
        <div class=" form-group text-center">
            <button type="button" class="btn btn-sm btn-primary mb-2" data-bs-dismiss="modal"><i class="oi oi-ban"></i> Volver</button>
        </div>
    </div>
</div>


